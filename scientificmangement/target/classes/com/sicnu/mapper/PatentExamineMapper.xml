    <delete id="delPatentExamineById" parameterType="int">
        delete from patent_examine
        where
        patent_id = #{patent_id}
    </delete>
    <delete id="delPatentExamine" parameterType="int">
        delete from patent_examine
        where
        leader_id = #{leader_id}
        and
        patent_name =#{patent_name}
    </delete>
