    <insert id="addPatentExamine" parameterType="PatentExamine">
        insert into patent_examine(
        patent_name,
        leader_id,
        pt_id,
        pr_id,
        ps_id,
        aod_id,
        application_number,
        application_time,
        public_number,
        public_time,
        authorization_number,
        authorization_time,
        examine_status,
        apply_time,
        reviewer_id
        ) values(
        #{patent_name},
        #{leader_id},
        #{pt_id},
        #{pr_id},
        #{ps_id},
        #{aod_id},
        #{application_number},
        #{application_time},
        #{public_number},
        #{public_time},
        #{authorization_number},
        #{authorization_time},
        #{examine_status},
        #{apply_time},
        #{reviewer_id}
        )
    </insert>
