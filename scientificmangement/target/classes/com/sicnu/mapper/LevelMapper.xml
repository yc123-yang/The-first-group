<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sicnu.mapper.LevelMapper">
    <insert id="addRole" parameterType="Level">
        insert into role(level_name) values(#{level_name})
    </insert>
    <delete id="delRole" parameterType="Level">
        delete from role where level_id = #{level_id}
    </delete>
</mapper>