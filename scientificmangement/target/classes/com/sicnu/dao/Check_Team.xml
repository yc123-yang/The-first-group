<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sicnu.dao.Check_TeamDao">

    <insert id="checkProjectTeam" parameterType="Check_Team">
        insert into check_team(
        project_id,
        user_id,
        join_time
        ) values (
        #{project_id},
        #{user_id},
        #{join_time}
        )
    </insert>


</mapper>