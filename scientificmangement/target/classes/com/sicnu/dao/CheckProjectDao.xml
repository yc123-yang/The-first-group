<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sicnu.dao.CheckProjectDao">

    <insert id="addProject" parameterType="CheckProject">
        insert into check_project(
        project_stage,
        project_type,
        leader_id,
        project_name,
        project_abstract,
        declaration,
        apply_time
        ) values(
        #{project_stage},
        #{project_type},
        #{leader_id},
        #{project_name},
        #{project_abstract},
        #{declaration},
        #{apply_time}
        )
    </insert>

    <select id="findAllProject" parameterType="string" resultType="CheckProject">
		select * from check_project
	</select>

    <delete id="delCheckProjectById" parameterType="CheckProject">
        delete from check_project
        where
        project_id = #{project_id}
    </delete>

</mapper>