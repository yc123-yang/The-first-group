<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sicnu.dao.CheckTeamDao">

    <insert id="checkProjectTeam" parameterType="CheckTeam">
        insert into check_team(
        project_id,
        user_id,
        join_time
        ) values (
        #{project_id},
        #{user_id},
        #{join_time}
        )
    </insert>
    <select id="selectCheckUser" resultType="CheckTeam">
        select * from check_team
        where
        project_id = #{project_id}
    </select>
    <delete id="delCheckTeamUser" parameterType="CheckTeam">
        delete from check_team
        where
        user_id =#{user_id}
    </delete>
</mapper>