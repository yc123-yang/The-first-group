    <delete id="delAwardExamineById" parameterType="int">
        delete from award_examine
        where
        award_id = #{award_id}
    </delete>
    <delete id="delAwardExamine" parameterType="int">
        delete from award_examine
        where
        leader_id = #{leader_id}
        and
        award_name = #{award_name}
    </delete>
