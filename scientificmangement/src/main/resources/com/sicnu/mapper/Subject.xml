<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.sicnu.mapper.SubjectMapper">
    <insert id="addRole" parameterType="Subject">
        insert into subject(subject_name) values(#{subject_name})
    </insert>
    <delete id="delRole" parameterType="Subject">
        delete from subject where subject_id = #{subject_id}
    </delete>
</mapper>